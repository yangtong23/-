.nickname{
  color: black;
}
.top{
  background: white;
  border-top:1px solid  #e7e6e6; 
  border-bottom:1px solid  #e7e6e6; 
  font-size: 26rpx;
  color: #797878;
  padding: 40rpx;
 
}
.top>image:first-child{
   width: 120rpx;
   border-radius: 50%;
   vertical-align: middle;
   margin-right: 20rpx;
}
.icon_right{
  width: 28rpx;
  vertical-align: middle;
  margin-left: 6rpx;
}

.order{
  display: flex;
  justify-content: space-between;
  align-items: center;
  line-height: 40rpx;
  padding: 30rpx 24rpx;
}
.myorder{
  font-size: 30rpx;
  vertical-align: middle;
}
.icon_right2{
  width: 30rpx;
  vertical-align: middle;
}
.findAll{
  font-size: 24rpx;
  padding-right: 4rpx;
  vertical-align: middle;
  color: #aaaaaa;
}
.orderList{
  display: flex;
   align-items: center;
   justify-content: space-between;
   text-align: center;
   padding: 20rpx 50rpx 30rpx;
}
.orderImg{
  width: 60rpx;
}
.orderText{
  font-size: 24rpx;
  color:#7e7d7d;
}
.space_view{
   width: 100%;
  height: 20rpx;
  background: #f1f0f0;
}
.houseList{
  display: flex;
   align-items: center;
   justify-content: space-around;
   text-align: center;
   padding: 30rpx 30rpx 30rpx;
}
.houseImg{
  width: 60rpx;
}
.houseText{
  font-size: 26rpx;
  color:#302f2f;
}

.welfareList{
  display: flex;
  flex-wrap: wrap;
   align-items: center;
   justify-content: space-between;
   text-align: center;
   padding: 60rpx 0rpx 30rpx ;
}
.welfareImg{
  width: 60rpx;
  margin-bottom: 12rpx;
}
.welfareText{
  font-size: 24rpx;
  color:#696767;
}
.welfareView{
  width: 25%;
  margin-bottom: 40rpx;
}